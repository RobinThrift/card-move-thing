{
    "rules": {
        "class-name": true,
        "comment-format": [true, "check-space", "check-lowercase"],
        "curly": true,
        "indent": [true, "spaces"],
        "no-consecutive-blank-lines": true,
        "no-console": true,
        "no-construct": true,
        "no-duplicate-key": true,
        "no-empty": false,
        "no-shadowed-variable": true,
        "no-trailing-whitespace": true,
        "no-unreachable": true,
        "no-unused-expression": true,
        "no-unused-variable": "react",
        "no-use-before-declare": true,
        "no-var-keyword": true,
        "one-line": [true, "check-catch", "check-else", "check-whitespace"],
        "quotemark": [false],
        "radix": true,
        "semicolon": true,
        "trailing-comma": [true, "never"],
        "triple-equals": true,
        "typedef-whitespace": [true, "space"],
        "variable-name": [false],
        "whitespace": [true, "check-branch", "check-decl", "check-operator", "check-module", "check-type", "check-typecast"]
    }
}
